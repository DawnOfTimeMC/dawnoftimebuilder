{
	"multipart": [
		{
			"when": { "axis": "y" },
			"apply": { "model": "dawnoftimebuilder:block/general/spruce_pergola_y"}
		},
		{
			"when": {"OR": [
				{"axis": "x", "subaxis_z": "false" },
				{"axis": "y", "subaxis_x": "true", "subaxis_z": "false" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/general/spruce_pergola_x"}
		},
		{
			"when": {"OR": [
				{"axis": "z", "subaxis_x": "false" },
				{"axis": "y", "subaxis_z": "true", "subaxis_x": "false" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/general/spruce_pergola_x", "y": 90}
		},
		{
			"when": {"OR": [
				{"axis": "x", "subaxis_z": "true" },
				{"axis": "z", "subaxis_x": "true" },
				{"subaxis_z": "true", "subaxis_x": "true" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/general/spruce_pergola_x_z"}
		},
		{
			"when": { "bottom": "true" },
			"apply": { "model": "dawnoftimebuilder:block/general/spruce_pergola_bottom"}
		},
		{
			"when": { "climbing_plant": "grape", "age": "0", "axis": "y"},
			"apply": [
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0"},
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 90 },
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 180 },
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 270 }
			]
		},
		{
			"when": { "climbing_plant": "grape", "age": "1", "axis": "y"},
