{
	"multipart": [
		{
			"when": { "axis_y": "true" },
			"apply": { "model": "dawnoftimebuilder:block/german/waxed_oak_pergola_y"}
		},
		{
			"when": {"axis_x": "true", "axis_z": "false" },
			"apply": { "model": "dawnoftimebuilder:block/german/waxed_oak_pergola_x"}
		},
		{
			"when": {"axis_x": "false", "axis_z": "true" },
			"apply": { "model": "dawnoftimebuilder:block/german/waxed_oak_pergola_x", "y": 90}
		},
		{
			"when": {"axis_x": "true", "axis_z": "true" },
			"apply": { "model": "dawnoftimebuilder:block/german/waxed_oak_pergola_x_z"}
		},
		{
			"when": { "bottom": "true" },
			"apply": { "model": "dawnoftimebuilder:block/german/waxed_oak_pergola_bottom"}
		},
		{
			"when": { "climbing_plant": "grape", "age": "0", "axis_y" : "true"},
			"apply": [
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0"},
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 90 },
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 180 },
				{ "model": "dawnoftimebuilder:block/general/grape/grape_y_stage_0", "y": 270 }
			]
		},
		{
			"when": { "climbing_plant": "grape", "age": "1", "axis_y" : "true"},
